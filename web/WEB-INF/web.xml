<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SevletGetionPersonnel</servlet-name>
        <servlet-class>org.Projet.servletes.administrateur.SevletGetionPersonnel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SevletGetionPersonnel</servlet-name>
        <url-pattern>/GestionPersonnel</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletGestionChambre</servlet-name>
        <servlet-class>org.Projet.servletes.administrateur.ServletGestionChambre</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletGestionChambre</servlet-name>
        <url-pattern>/GestionChambre</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletGestionPatient</servlet-name>
        <servlet-class>org.Projet.servletes.agentParamedical.ServletGestionPatient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletGestionPatient</servlet-name>
        <url-pattern>/GestionPatient</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletAuthentification</servlet-name>
        <servlet-class>org.Projet.servletes.general.ServletAuthentification</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAuthentification</servlet-name>
        <url-pattern>/Authentification</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletAccueil</servlet-name>
        <servlet-class>org.Projet.servletes.agentParamedical.ServletAccueil</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAccueil</servlet-name>
        <url-pattern>/AccueilAgentParamedicale</url-pattern>
    </servlet-mapping>


    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>

</web-app>