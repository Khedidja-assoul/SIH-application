        <servlet-class>org.Projet.servletes.agentBlocOperatoire.ServletAccueilAgentBloc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAccueilAgentBloc</servlet-name>
        <url-pattern>/AccueilBlocOperatoire</url-pattern>
    </servlet-mapping>

    <!-- Patient -->
    <servlet>
        <servlet-name>ServletAccueilPatient</servlet-name>
        <servlet-class>org.Projet.servletes.patient.ServletAccueilPatient</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAccueilPatient</servlet-name>
        <url-pattern>/AccueilPatient</url-pattern>
    </servlet-mapping>

    <!--test servlet -->
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>org.Projet.servletes.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <!-- configuration-->

    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>

</web-app>